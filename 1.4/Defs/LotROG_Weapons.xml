 <!-- Gundabad Orc Dagger -->
  <ThingDef ParentName="BaseMeleeWeapon_Sharp_Quality">
    <defName>LotROG_Gundabad_Dagger</defName>
    <label>gundabad dagger</label>
    <techLevel>Neolithic</techLevel>
    <description>The gundabad-dagger, A handle for holding and a sharp edge for cutting.</description>
    <graphicData>
      <texPath>Weapons/Gundabad/gundabad-dagger</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <uiIconScale>1.6</uiIconScale>
    <weaponTags>
      <li>GundabadKnife</li>
      <li>GundabadMelee</li>
    </weaponTags>
    <weaponClasses>
      <li>Neolithic</li>
    </weaponClasses>
    <costStuffCount>30</costStuffCount>
    <statBases>
      <WorkToMake>1800</WorkToMake>
      <Mass>0.5</Mass>
    </statBases>
    <equippedAngleOffset>-65</equippedAngleOffset>
    <stuffCategories>
      <li>Metallic</li>
    </stuffCategories>
    <burnableByRecipe>false</burnableByRecipe>
    <tools>
      <li>
        <label>handle</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>9</power>
        <cooldownTime>2</cooldownTime>
      </li>
      <li>
        <label>blade</label>
        <capacities>
          <li>Cut</li>
        </capacities>
        <power>12</power>
        <cooldownTime>1.5</cooldownTime>
      </li>
      <li>
        <label>point</label>
        <capacities>
          <li>Stab</li>
        </capacities>
        <power>13</power>
        <cooldownTime>2</cooldownTime>
      </li>
    </tools>
    <recipeMaker>
      <recipeUsers>
        <li>CraftingSpot</li>
      </recipeUsers>
      <displayPriority>310</displayPriority>
    </recipeMaker>
  </ThingDef>

