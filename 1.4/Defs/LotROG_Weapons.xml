 <!-- Goblin Bow -->
  <ThingDef ParentName="BaseWeaponNeolithic">
    <defName>LotROG_Goblin_Bow</defName>
    <label>short bow</label>
    <description>A simple short bow made from wood.</description>
    <possessionCount>1</possessionCount>
    <graphicData>
      <texPath>Weapons/Goblin/goblin-bow</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <soundInteract>Bow_Small</soundInteract>
    <costList>
      <WoodLog>30</WoodLog>
    </costList>
    <statBases>
      <WorkToMake>2400</WorkToMake>
      <Mass>0.8</Mass>
      <AccuracyTouch>0.75</AccuracyTouch>
      <AccuracyShort>0.65</AccuracyShort>
      <AccuracyMedium>0.45</AccuracyMedium>
      <AccuracyLong>0.25</AccuracyLong>
      <RangedWeapon_Cooldown>1.65</RangedWeapon_Cooldown>
    </statBases>
    <weaponTags>
      <li>LotROG_Goblin_Bow</li>
      <li>LotROG_Goblin_Ranged</li>
    </weaponTags>
    <weaponClasses>
      <li>Ranged</li>
      <li>RangedLight</li>
    </weaponClasses>
    <recipeMaker>
      <recipeUsers>
        <li>CraftingSpot</li>
      </recipeUsers>
      <skillRequirements>
        <Crafting>2</Crafting>
      </skillRequirements>
      <displayPriority>510</displayPriority>
    </recipeMaker>
    <verbs>
      <li>
        <verbClass>Verb_Shoot</verbClass>
        <hasStandardCommand>true</hasStandardCommand>
        <defaultProjectile>Arrow_Short</defaultProjectile>
        <warmupTime>1.35</warmupTime>
        <range>22.9</range>
        <soundCast>Bow_Small</soundCast>
      </li>
    </verbs>
    <tools>
      <li>
        <label>limb</label>
        <capacities>
          <li>Blunt</li>
          <li>Poke</li>
        </capacities>
        <power>9</power>
        <cooldownTime>2</cooldownTime>
      </li>
    </tools>
    <rotateInShelves>false</rotateInShelves>
  </ThingDef>

  <Gunplay.GunPropDef ParentName="GunplayArrow">
      <defName>LotROG_Goblin_Bow</defName>
  </Gunplay.GunPropDef>


