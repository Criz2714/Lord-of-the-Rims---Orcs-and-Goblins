<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!-- Gundabad -->
  <GeneDef ParentName="GeneEarsBase">
    <defName>LotROG_Gene_Ears_GundabadOrc</defName>
    <label>Gundabad orc ears</label>
    <description>Carriers of this gene have Gundabad orc ears.</description>
    <iconPath>UI/Icons/Genes/Gene_EarPointed</iconPath>
    <randomChosen>false</randomChosen>
    <renderNodeProperties>
      <li>
        <nodeClass>PawnRenderNode_AttachmentHead</nodeClass>
        <workerClass>PawnRenderNodeWorker_FlipWhenCrawling</workerClass>
        <texPath>GundabadOrc/Ears/GundabadEar</texPath>
        <colorType>Skin</colorType>
        <parentTagDef>Head</parentTagDef>
        <useSkinShader>true</useSkinShader>
        <useRottenColor>true</useRottenColor>
        <rotDrawMode>Fresh, Rotting</rotDrawMode>
        <drawData>
          <defaultData>
            <layer>70</layer>
          </defaultData>
        </drawData>
      </li>
    </renderNodeProperties>
    <symbolPack>
      <prefixSymbols>
        <li><symbol>point</symbol></li>
        <li><symbol>sharp</symbol></li>
      </prefixSymbols>
      <suffixSymbols>
        <li><symbol>ear</symbol></li>
        <li><symbol>point</symbol></li>
      </suffixSymbols>
    </symbolPack>
  </GeneDef>

