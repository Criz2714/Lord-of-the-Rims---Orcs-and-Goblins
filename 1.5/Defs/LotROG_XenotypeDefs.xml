<?xml version="1.0" encoding="utf-8" ?>
<Defs>

 <!-- Goblin - Misty Mountain -->
  <XenotypeDef>
    <defName>LotROG_Xeno_GoblinMistyMountain</defName>
    <label>Misty Mountain Goblin</label>
    <description>With gray skin, they thrive in cramped and dark spaces where combat and travel happen over short distances. They suffer from a sensitivity to light, slow speed and poor eyesight at distance.</description>
    <iconPath>UI/Icons/Xenotypes/Baseliner</iconPath>
    <inheritable>true</inheritable>
    <genes>
      <li>Skin_LightGray</li>
	  <li>Body_Thin</li>
      <li>LotROG_Gene_Ears_MistyGoblin</li>
      <li>DarkVision</li>
      <li>Nearsighted</li>
      <li>UVSensitivity_Intense</li>
      <li>Aggression_Aggressive</li>
      <li>LotROG_Gene_CaveDweller</li>
      <li>LotROG_Gene_Wimp</li>
      <li>LotROG_Gene_CreepyBreathing</li>
      <li>LotROG_Gene_Bloodlust</li>
    </genes>
  </XenotypeDef>

